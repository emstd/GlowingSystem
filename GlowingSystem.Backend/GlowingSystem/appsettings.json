{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;database=GlowingSystemDb;trusted_connection=true;TrustServerCertificate=True"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "Application": "GlowingSystem"
    }
  },
  "AllowedHosts": "*"
}
